{
  "name": "edotor.net",
  "author": "Niklas Mollenhauer",
  "license": "MIT",
  "version": "1.7.1",
  "description": "Online Graphviz fiddle with language support",
  "private": true,
  "type": "module",
  "scripts": {
    "build:new": "vite build",
    "build:prod": "NODE_OPTIONS=--openssl-legacy-provider webpack -p",
    "build:dev": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode development",
    "watch": "NODE_OPTIONS=--openssl-legacy-provider webpack --watch --mode development",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "bootstrap": "4.6.2",
    "dot-language-support": "1.7.0",
    "jquery": "3.6.0",
    "monaco-editor": "0.15.6",
    "monaco-languageclient": "0.10.2",
    "popper.js": "1.16.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-spinners": "0.17.0",
    "react-split-pane": "0.1.92",
    "svg-pan-zoom": "3.6.2",
    "viz.js": "2.1.2"
  },
  "devDependencies": {
    "@types/react": "18.3.21",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "sass-embedded": "^1.88.0",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-static-copy": "^3.0.0"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
